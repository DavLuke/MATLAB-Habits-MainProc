allConditions = nan(numBlocks, (trialsxblock));
for i =1:numBlocks
    
rightStimuli = nan(1, trialsxblock);
yy = trialsxblock/2;
HV = 1;
LV = 2;

rightStimuli = [HV, LV];
rightStimuli =repmat(rightStimuli,1,yy);


ww(1,1:trialsxblock) = rightStimuli;
ww_rand = ww(randperm(length(ww)));

    
allConditions(i,:) = ww_rand(:);

end
